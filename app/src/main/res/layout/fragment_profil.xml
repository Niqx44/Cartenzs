<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfilFragment"
    android:background="@drawable/maintop">

    <LinearLayout
        android:clipChildren="false"
        android:clipToPadding="false"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:clipChildren="false"
                    android:clipToPadding="false"
                    android:orientation="vertical">

                <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="65dp"
                        android:layout_marginBottom="140dp"
                        android:fontFamily="sans-serif-black"
                        android:text="Profile"
                        android:textAlignment="center"
                        android:textColor="#FFFFFF"
                        android:textSize="26sp" />

                <RelativeLayout
                        android:id="@+id/rmibsisq1a"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:clipChildren="false"
                        android:clipToPadding="false">

                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:clipChildren="false"
                            android:clipToPadding="false"
                            android:orientation="vertical">

                            <LinearLayout
                                android:id="@+id/rmqkjehakhlj"
                                android:layout_width="match_parent"
                                android:layout_height="700dp"
                                android:background="@drawable/tl50tr50bffffff"
                                android:layout_gravity="center_horizontal"
                                android:orientation="vertical"
                                android:paddingTop="64dp">

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_marginHorizontal="79dp"
                                    android:layout_marginBottom="1dp"
                                    android:fontFamily="sans-serif-medium"
                                    android:text="Muh. Farhan Rizki"
                                    android:textAlignment="center"
                                    android:textColor="#67BD64"
                                    android:textSize="26sp" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_marginHorizontal="143dp"
                                    android:layout_marginBottom="23dp"
                                    android:text="@string/muhfarhanrizki"
                                    android:textAlignment="center"
                                    android:textColor="#949793"
                                    android:textSize="13sp" />

                                <Button
                                    android:id="@+id/edit"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_marginBottom="40dp"
                                    android:background="@drawable/cr10lr27067bd642f572e"
                                    android:fontFamily="sans-serif-black"
                                    android:text="Edit Profile"
                                    android:textColor="@color/white"
                                    android:textSize="12sp"
                                    tools:ignore="TouchTargetSizeCheck" />

                                <LinearLayout
                                    android:id="@+id/urprofil"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginHorizontal="58dp"
                                    android:layout_marginBottom="25dp"
                                    android:gravity="center_vertical"
                                    android:minHeight="48dp"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:layout_width="30dp"
                                        android:layout_height="30dp"
                                        android:layout_marginEnd="24dp"
                                        android:scaleType="fitXY"
                                        android:src="@drawable/yourprofil" />

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:fontFamily="sans-serif-black"
                                        android:text="Your Profile"
                                        android:textColor="#67BD64"
                                        android:textSize="16sp" />
                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/setting"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginHorizontal="58dp"
                                    android:layout_marginBottom="25dp"
                                    android:gravity="center_vertical"
                                    android:minHeight="48dp"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:layout_width="32dp"
                                        android:layout_height="32dp"
                                        android:layout_marginEnd="22dp"
                                        android:scaleType="fitXY"
                                        android:src="@drawable/setting" />

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:fontFamily="sans-serif-black"
                                        android:text="Pengaturan"
                                        android:textColor="#67BD64"
                                        android:textSize="16sp" />
                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/informasi"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginHorizontal="58dp"
                                    android:layout_marginBottom="25dp"
                                    android:gravity="center_vertical"
                                    android:minHeight="48dp"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:layout_width="30dp"
                                        android:layout_height="30dp"
                                        android:layout_marginEnd="24dp"
                                        android:scaleType="fitXY"
                                        android:src="@drawable/information" />

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:fontFamily="sans-serif-black"
                                        android:text="Information"
                                        android:textColor="#67BD64"
                                        android:textSize="16sp" />
                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/privacy"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginHorizontal="58dp"
                                    android:layout_marginBottom="25dp"
                                    android:gravity="center_vertical"
                                    android:minHeight="48dp"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:layout_width="30dp"
                                        android:layout_height="30dp"
                                        android:layout_marginEnd="24dp"
                                        android:scaleType="fitXY"
                                        android:src="@drawable/policy" />

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:fontFamily="sans-serif-black"
                                        android:text="Privacy Policy"
                                        android:textColor="#67BD64"
                                        android:textSize="16sp" />
                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/keluar"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginHorizontal="58dp"
                                    android:layout_marginBottom="25dp"
                                    android:clickable="true"
                                    android:focusable="true"
                                    android:gravity="center_vertical"
                                    android:minHeight="48dp"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:layout_width="30dp"
                                        android:layout_height="30dp"
                                        android:layout_marginEnd="24dp"
                                        android:scaleType="fitXY"
                                        android:src="@drawable/logout" />

                                    <TextView
                                        android:id="@+id/textout"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:fontFamily="sans-serif-black"
                                        android:text="Keluar"
                                        android:textColor="#67BD64"
                                        android:textSize="16sp"
                                        tools:ignore="TouchTargetSizeCheck" />
                                </LinearLayout>


                            </LinearLayout>
                        </LinearLayout>

                    <LinearLayout
                            android:id="@+id/r5en4j7f1w69"
                            android:layout_marginTop="-115dp"
                            android:layout_marginHorizontal="130dp"
                            android:layout_width="wrap_content"
                            android:layout_height="153dp"
                            android:background="@drawable/userprofil1"
                            android:clipChildren="false"
                            android:clipToPadding="false"
                            android:orientation="vertical"></LinearLayout>
                    </RelativeLayout>
                </LinearLayout>
        </ScrollView>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>